generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model produk {
  id         BigInt   @id @default(autoincrement())
  name       String   @unique @db.VarChar(255)
  harga      Decimal  @db.Decimal(10, 2) 
  created_at DateTime @default(now())

}